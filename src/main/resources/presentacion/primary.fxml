<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentacion.PrimaryController">
   <children>
      <AnchorPane>
         <children>
            <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
              <tabs>
                <Tab text="Clientes">
                  <content>
                    <AnchorPane>
                         <fx:include fx:id="layoutClientes" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" source="clientes.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                    </AnchorPane>
                  </content>
                </Tab>
                <Tab text="Productos">
                  <content>
                    <AnchorPane>
                         <fx:include fx:id="layoutProductos" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" source="productos.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                    </AnchorPane>
                  </content>
                </Tab>
                  <Tab text="Configuracion">
                     <content>
                        <AnchorPane>
                            <fx:include fx:id="layoutAppConfig" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" source="appConfig.fxml" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                        </AnchorPane>
                     </content>
                  </Tab>
              </tabs>
            </TabPane>
         </children>
      </AnchorPane>
   </children>
</VBox>
